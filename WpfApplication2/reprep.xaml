            <TextSearch.TextPath>CARD_ID</TextSearch.TextPath>
            <ComboBox.ItemTemplate >
                <DataTemplate>
                    <TextBlock Text="{Binding CARD_ID}"/>
                </DataTemplate>
            </ComboBox.ItemTemplate>
        </ComboBox>
